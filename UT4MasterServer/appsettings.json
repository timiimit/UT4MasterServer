{
	"ApplicationSettings": {
		"DatabaseConnectionString": "mongodb://devroot:devroot@mongo:27017",
		"DatabaseName": "ut4master",
		"AllowPasswordGrantType": true,
		"ProxyClientIPHeader": "X-Forwarded-For",
		"ProxyServers": ["172.20.0.1"]
	},
	"StatisticsSettings": {
		"DeleteOldStatisticsTimeZone": "Central European Standard Time",
		"DeleteOldStatisticsHour": 10,
		"DeleteOldStatisticsBeforeDays": 90,
		"MergeOldStatisticsTimeZone": "Central European Standard Time",
		"MergeOldStatisticsHour": 10
	},
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"Serilog": {
		"MinimumLevel": {
			"Default": "Warning",
			"Override": {
				"System": "Warning",
				"Microsoft.AspNetCore": "Warning"
			}
		}
	},
	"AllowedHosts": "*",
	"Kestrel": {
		"Endpoints": {
			"Http": {
				"Url": "http://0.0.0.0:8080"
			}
		}
	}
}
