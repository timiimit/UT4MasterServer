# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.{cs,vb}]
tab_width = 4
indent_size = 4
indent_style = tab

[UT4MasterServer/**.json]
tab_width = 4
indent_size = 4
indent_style = tab

[*.{md,yml}]
indent_style = space
indent_size = 2
